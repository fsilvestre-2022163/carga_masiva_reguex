generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Acceso_Planillas {
  id            Int          @id @default(autoincrement())
  idEmpresa     String?      @db.VarChar(70)
  idColaborador String?      @db.VarChar(70)
  accesos       String?      @db.MediumText
  Empresa       Empresa?     @relation(fields: [idEmpresa], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "Acceso_Planillas_ibfk_1")
  Colaborador   Colaborador? @relation(fields: [idColaborador], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "Acceso_Planillas_ibfk_2")

  @@index([idColaborador], map: "idColaborador")
  @@index([idEmpresa], map: "idEmpresa")
}

model Anticipos {
  id                 Int       @id @default(autoincrement())
  idPlaza            Int?
  idPlanilla         Int?
  anticipo           Decimal?  @db.Decimal(10, 2)
  interes            Decimal?  @db.Decimal(10, 2)
  inicioPeriodoCobro DateTime? @db.DateTime(0)
  saldo              Decimal?  @db.Decimal(10, 2)
  estado             Boolean?
  Plaza              Plaza?    @relation(fields: [idPlaza], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "Anticipos_ibfk_1")

  @@index([idPlaza], map: "idPlaza")
}

model AnticiposPendientes {
  id         Int      @id @default(autoincrement())
  idAnticipo Int?
  Saldo      Decimal? @db.Decimal(10, 2)
  cuota      Decimal? @db.Decimal(10, 2)
  estado     Boolean?
}

model Area_Departamento {
  id                 String   @id @db.VarChar(70)
  nombreDepartamento String?  @db.VarChar(100)
  idEmpresa          String?  @db.VarChar(70)
  Empresa            Empresa? @relation(fields: [idEmpresa], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "Area_Departamento_ibfk_1")

  @@index([idEmpresa], map: "idEmpresa")
}

model Banco {
  id                Int                 @id @default(autoincrement())
  nombre            String?             @db.VarChar(255)
  Planilla          Planilla[]
  PlanillasB14      PlanillasB14[]
  Plaza             Plaza[]
  PlazaPlanillaPago PlazaPlanillaPago[]
}

model Centro {
  id                       Int           @id @default(autoincrement())
  nombreCentro             String?       @db.VarChar(100)
  direccionCentro          String?       @db.VarChar(100)
  zona                     Int?
  telefonosCentro          String?       @db.VarChar(40)
  faxCentro                String?       @db.VarChar(40)
  nombreContactoCentro     String?       @db.VarChar(50)
  correoCentro             String?       @db.VarChar(50)
  departamentoCentro       Int?
  municipioCentro          Int?
  codigoActividadEconomica String?       @db.VarChar(45)
  idEmpresa                String?       @db.VarChar(70)
  correlativo              Int?
  Departamento             Departamento? @relation(fields: [departamentoCentro], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "Centro_ibfk_1")
  Municipio                Municipio?    @relation(fields: [municipioCentro], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "Centro_ibfk_2")
  Empresa                  Empresa?      @relation(fields: [idEmpresa], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "Centro_ibfk_3")
  Plaza                    Plaza[]

  @@index([departamentoCentro], map: "departamentoCentro")
  @@index([idEmpresa], map: "idEmpresa")
  @@index([municipioCentro], map: "municipioCentro")
}

